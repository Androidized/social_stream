<ul class="list-group">
  <% list.each do |client| %>
    <li class="list-group-item">
      <%= link_to client, :class => "logo-app" do %>
        <%= image_tag client.logo.url(:small) %>
      <% end %>

      <div class="data">
        <div class="name">
          <%= link_to client.name, client %>
        </div>
        <div class="url">
          <%= link_to client.url, client.url, target: '_blank' %>
        </div>
      </div>
      <div class="btn-group">
        <button class="btn btn-default disabled">
          5 users
        </button>
          
          <button class="btn btn-default disabled">
            15 authorized
          </button>
      </div>
     
    </li>
  <% end %>

<%= javascript_tag do %>
  SocialStream.SiteClient.index();
<% end %>
</ul>

